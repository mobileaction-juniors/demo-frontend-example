{
    "name": "@mobileaction/eslint-config",
    "version": "2.2.0",
    "description": "Mobile Action eslint config",
    "author": "info@mobileaction.co",
    "scripts": {
        "lint": "eslint . -c index.js",
        "lint-report": "export RPATH=../../reports/eslint; mkdir -p $RPATH && yarn lint -f junit > $RPATH/eslint-config.xml",
        "test": "yarn lint-report"
    },
    "peerDependencies": {
        "eslint": "^8.2.0",
        "eslint-plugin-gridsome": "^1.6.2"
    },
    "dependencies": {
        "@babel/eslint-parser": "^7.16.3",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-vue": "^8.0.3",
        "vue-eslint-parser": "^8.0.1"
    },
    "bugs": {
        "url": "https://github.com/mobileaction/ui-modules/issues"
    },
    "homepage": "https://github.com/mobileaction/ui-modules#readme",
    "keywords": [
        "vue",
        "js",
        "eslint",
        "preset"
    ],
    "eslintConfig": {
        "extends": [
            "./index.js"
        ]
    },
    "babel": {
        "presets": [
            [
                "@vue/app"
            ]
        ]
    },
    "license": "UNLICENSED",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mobileaction/ui-modules.git"
    },
    "gitHead": "ffdcd931d59124e5a0c5343a380ca98dec2b7fd3",
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@vue/babel-preset-app": "^4.5.15"
    }
}
